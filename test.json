{
  "title": "Test",
  "rules": [
    {
          "description": "capslock tap&hold as enter/hyperkey",
          "manipulators": [
            {
              "type": "basic",
              "from": {
                "key_code": "caps_lock",
                "modifiers": {
                  "mandatory": ["shift"],
                  "optional": ["caps_lock"]
                }
              },
              "to": [
                {
                  "key_code": "caps_lock"
                }
              ]
            },
            {
              "type": "basic",
              
              "from": {
                "key_code": "caps_lock",
                "modifiers": {
                  "optional": ["any"]
                }
              },
              "to": [
                {
                  "key_code": "fn"
                }
              ],
              "to_if_alone": [
                {
                  "key_code": "return_or_enter"
                }
              ]
            }
          ]
    },
    {
      "description": "double tap HK + i/k for 10times ip/down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
                "key_code": "up_arrow"
            },
            {
                "key_code": "up_arrow"
            },
            {
                "key_code": "up_arrow"
            },
            {
                "key_code": "up_arrow"
            },
            {
                "key_code": "up_arrow"
            },
            {
                "key_code": "up_arrow"
            },
            {
                "key_code": "up_arrow"
            },
            {
                "key_code": "up_arrow"
            }
        ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "UpPressed",
                "value": 0
              }
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "UpPressed",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            },
            {
              "set_variable": {
                "name": "UpPressed",
                "value": 1
              }
            }
          ],
          "parameters": {
            "basic.to_delayed_action_delay_milliseconds": 150
          },
          "to_delayed_action": {
            "to_if_invoked": [
              {
                "set_variable": {
                  "name": "UpPressed",
                  "value": 0
                }
              }
            ]
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
                "key_code": "down_arrow"
            },
            {
                "key_code": "down_arrow"
            },
            {
                "key_code": "down_arrow"
            },
            {
                "key_code": "down_arrow"
            },
            {
                "key_code": "down_arrow"
            },
            {
                "key_code": "down_arrow"
            },
            {
                "key_code": "down_arrow"
            },
            {
                "key_code": "down_arrow"
            },
            {
                "key_code": "down_arrow"
            }
        ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "DownPressed",
                "value": 0
              }
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "DownPressed",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            },
            {
              "set_variable": {
                "name": "DownPressed",
                "value": 1
              }
            }
          ],
          "parameters": {
            "basic.to_delayed_action_delay_milliseconds": 150
          },
          "to_delayed_action": {
            "to_if_invoked": [
              {
                "set_variable": {
                  "name": "DownPressed",
                  "value": 0
                }
              }
            ]
          }
        }
      ]
    },
