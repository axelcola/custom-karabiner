    {
      "description": "HyK = u/o as back/forward for visual studio code",
      "manipulators": [
        {
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "hyphen",
              "modifiers": ["left_control"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "hyphen",
              "modifiers": ["left_control", "left_shift"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "'v' w hyper as delete line",
      "manipulators": [
        {
          "from": {
            "key_code": "v",
            "modifiers": {
              "optional": ["any"],
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": "left_command"
            },
